<div>
    <div>
        <h3>Registration</h3>
    </div>
    <form id="registerForm">
        <div>
            <label for="First name">First name</label>
            <input type="first_name" id="first_name" name="first_name" required>
            <label for="Last name">Last name</label>
            <input type="last_name" id="last_name" name="last_name" required>
            <label for="Age">Age</label>
            <input type="age" id="age" name="age" required>
            <label for="Email">Email</label>
            <input type="email" id="email" name="email" required>
            <label for="Password">Password</label>
            <input type="password" id="password" name="password" required>
        </div>
        <div>
            <button id="btnRegister" type="submit">Sign up</button>
        </div>
        <div>
            <h3>Or..</h3>
            <a href="/api/users/github"><p>Sign Up with Github</p></a>
        </div>
        <div>
            <p>If you already have an account, <a href="/">Sign in</a>.</p>
        </div>
    </form>
</div>
</div>